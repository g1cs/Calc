; /* Start:"a:4:{s:4:"full";s:91:"/local/templates/index/components/bitrix/system.auth.form/.default/script.js?15601600231061";s:6:"source";s:76:"/local/templates/index/components/bitrix/system.auth.form/.default/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(function () {

	var loginForm = $('.login-form'),
		isSubmitLocked = false;

	loginForm.each(function () {
		$(this).validate({
			rules: {
				'USER_LOGIN': {required: true},
				'USER_PASSWORD': {required: true}
			},
			submitHandler: function (form) {
				if (isSubmitLocked) {
					return false;
				}
				isSubmitLocked = true;

				$.post('/local/ajax/authorize.php', $(form).serialize(), function (response) {
					if (response.status != 'ok') {
						$(form).find('.form-error')
							.html(response.message)
							.show();
						$(form).find(".pop-up__field-wrap")
							.children('input').addClass("pop-up--field-error");

						return false;
					}
					location.reload();

				}).always(function () {
					isSubmitLocked = false;
				});

				return false;
			}
		});
	});

	/*$(document).on('click', '.popup-modal-dismiss', function(e) {
		e.preventDefault();
		loginForm.validate().resetForm();
		loginForm.find('.form-error').hide();
		loginForm.find('.pop-up__field-wrap').children('input').removeClass("pop-up--field-error");
	});*/

});

/* End */
