; /* Start:"a:4:{s:4:"full";s:60:"/local/templates/index/js/directive-v-mask.js?15601600232390";s:6:"source";s:45:"/local/templates/index/js/directive-v-mask.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
(function (Vue) {
	function format(data, mask){
		if (!mask) return data;

		var maskStartRegExp = /^([^#ANX]+)/;

		if (data.length === 1 && maskStartRegExp.test(mask)) {
			data = maskStartRegExp.exec(mask)[0] + data;
		}

		var text = '';
		var cOffset = 0;

		for (var i = 0; i < mask.length; i++) {
			var n = mask.charAt(i);

			switch(n) {
				case '#': break;
				case 'A': break;
				case '?': break;
				case 'F': break;
				case 'N': break;
				case 'X': break;
				default: data = data.replace(n, '')
			}
		}

		for (var j = 0, x = 1; x && j < mask.length; j++) {
			var c = data.charAt(j - cOffset);
			var m = mask.charAt(j);

			switch (m) {
				case '#':
					if (/[0-9]/.test(c)) {
						text += c;
					} else {
						x = 0;
					}
					break;
				case 'A':
					if (/[a-z]/i.test(c)) {
						text += c;
					} else {
						x = 0;
					}
					break;
				case 'N':
					if (/[a-z0-9]/i.test(c)) {
						text += c;
					} else {
						x = 0;
					}
					break;
				case '?' :
					cOffset++;
					break;
				case 'F' :
					if (/[0-9,.]/.test(c)) {
						if (/[0-9]/.test(c)) {
							if (text.length === 2 && text.indexOf('.') === -1) {
								x = 0;
							} else {
								text += c;
							}
						} else {
							if (text.indexOf('.') !== -1) {
								x = 0;
							} else {
								text += '.';
							}
						}
					} else {
						x = 0;
					}
					break;
				case 'X':
					text += c;
					break;
				default:
					text += m;

					if (c && c !== m) {
						data = ' ' + data;
					}

					break;
			}
		}
		return text;
	}

	function updateValue(el, force) {
		var cValue = el.value;
		var pValue = el.getAttribute('data-previous-value') || '';
		var mask = el.getAttribute('data-mask');

		if (force || (cValue && cValue !== pValue && cValue.length > pValue.length)) {
			el.value = format(cValue, mask);

			var event = document.createEvent('HTMLEvents');
			event.initEvent('input', true, true);
			el.dispatchEvent(event);
		}

		el.setAttribute('data-previous-value', cValue);
	}

	Vue.directive('mask', {
		bind: function (el, binding) {
			el.setAttribute('data-mask', binding.value);

			updateValue(el, false);
		},
		componentUpdated: function (el, binding) {
			var isMaskChanged = binding.value !== binding.oldValue;
			if (isMaskChanged) {
				el.setAttribute('data-mask', binding.value);
			}

			updateValue(el, isMaskChanged);
		}
	});
})(Vue);

/* End */
;
; /* Start:"a:4:{s:4:"full";s:48:"/local/templates/index/js/menu.js?15601600232651";s:6:"source";s:33:"/local/templates/index/js/menu.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
/*global $*/

(function() {

	$(document).ready(function() {

		"use strict";

		var menuBotElements = {
			menuBot: $(".menu-bot__wrap"),
			menuBtn: $(".header__menu-btn"),
			menuBg: $(".pop-up-bg")
		};

		var menuTop = $(".menu-top");
		var activeClass = "_active";


		function addActiveClass(obj, flag) {
			$.each(obj, function(index, val) {
				if (flag) {
					val.removeClass(activeClass);
					menuBotElements.menuBg.removeAttr("style");
					return;
				}
				val.addClass(activeClass);
			});
		}

		menuBotElements.menuBtn.on("click", function(e) {
			e.preventDefault();
			if (!menuBotElements.menuBot.hasClass(activeClass)) {
				addActiveClass(menuBotElements);
				return;
			}
			addActiveClass(menuBotElements, true);
		});

		function toggleActiveSubItems() {

			toggleActiveSubItems.addArrowToSub = function() {
				$.each($('.menu-bot__link'), function(i, val) {
					if($(val).siblings('.menu-bot__sub').length > 0) {
						$(val).addClass('menu-bot__link--sub');
					}
				})
			};

			$('.menu-bot').find('.menu-bot__link').on('click', function(e) {
				$('.menu-bot__link').removeClass('active');
				$('.menu-bot__sub').removeClass('active');

				if(!$(e.target).siblings('.menu-bot__sub').length > 0) {
					return
				}
				e.preventDefault();

				if(toggleActiveSubItems.flag) {
					toggleActiveSubItems.flag = false;
					if(+$(e.target).attr('data-item') === +toggleActiveSubItems.itemIndex) {
						return
					}
				}
				if(!toggleActiveSubItems.itemIndex) {
					toggleActiveSubItems.itemIndex = $(e.target).attr('data-item');
				}
				toggleActiveSubItems.flag = true;
				e.preventDefault();
				$(e.target).toggleClass('active');
				$(e.target).siblings('.menu-bot__sub').toggleClass('active');
				toggleActiveSubItems.itemIndex = $(e.target).attr('data-item');
			})

		}

		toggleActiveSubItems();
		toggleActiveSubItems.addArrowToSub();

		function changeItemFontSize() {
			changeItemFontSize.getItemsCoutn = function() {
				return menuTop.children().length
			};
			if (changeItemFontSize.getItemsCoutn() > 3) {
				menuTop.children().children().animate({
					"font-size": "16px",
					"line-height": "19px"
				});
				return;
			}
		}

		changeItemFontSize();

		$(window).on('scroll', function () {
			if ($(window).scrollTop() > $('.header').height() + $('#bx-panel').height()) {
				$('.menu-bot__wrap').css({
					'position': 'fixed',
					'top': $('.header').height()
				})
			} else {
				$('.menu-bot__wrap').css({
					'position': 'absolute',
					'top': ''
				})
			}
		});


		$('.pop-up-bg').on('click', function() {
			addActiveClass(menuBotElements, true);
		});

	});


})();

/* End */
;
; /* Start:"a:4:{s:4:"full";s:64:"/local/templates/index/js/customelement_v-test.js?15601600237478";s:6:"source";s:49:"/local/templates/index/js/customelement_v-test.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
Vue.customElement('v-test', {
	props: ['code'],
	data: function () {
		return {
			endpoint: '',
			name: '',
			questions: {},
			questionsIds: [],
			sectionId: 0,
			//
			isComplete: false,
			activeQuestion: 0,
			questionsCount: 0,
			lastVisitedQuestion: 0,
			pointsCount: 0,
			debug: 0,
			resultId: null,
			resultTitle: '',
			resultText: '',
			//
			ts: Date.now()
		}
	},
	methods: {
		getData: function () {
			var _this = this;
			$.ajax({
				url: '/local/ajax/testing.php?section_id=' + _this.code + '&action=json',
				dataType: 'json',
				method: 'post',
				success: function (res) {
					_this.endpoint = res.ENDPOINT;
					_this.name = res.NAME;
					_this.questions = res.QUESTIONS;
					_this.questionsIds = res.QUESTIONS_IDS;
					_this.sectionId = res.SECTION_ID;
					//
					_this.questionsCount = res.QUESTIONS_IDS.length;
				}
			});
		},
		resetForm: function () {
			this.$refs.form.reset();
			this.isComplete = false;
			this.activeQuestion = 0;
			this.lastVisitedQuestion = 0;
			this.pointsCount = 0;
			this.resultTitle = '';
			this.resultText = '';
		},
		prevQuestion: function () {
			if (this.activeQuestion - 1 < 0) return;
			this.gotoQuestion(this.activeQuestion - 1);
		},
		nextQuestion: function () {
			if (this.activeQuestion + 1 >= this.questionsCount) return;
			this.gotoQuestion(this.activeQuestion + 1);
		},
		gotoQuestion: function (i) {
			this.activeQuestion = i;
			if (i > this.lastVisitedQuestion) {
				this.lastVisitedQuestion = i;
				if (this.lastVisitedQuestion == 1) {
					/**
					 * First answer test event
					 */
					console.log('first-answer-test-' + this.sectionId);
				}
			}

			var scrollto = $(window).width() <= 767 ? $('#scroll-to-mobile' + this.ts).offset().top : $('#scroll-to-here' + this.ts).offset().top;
			$('html, body').animate({
				scrollTop: scrollto
			}, 300);
		},
		isFirstQuestion: function () {
			return this.activeQuestion == 0;
		},
		isLastQuestion: function () {
			return this.activeQuestion == (this.questionsCount - 1);
		},
		processForm: function () {
			var _this = this;
			$(this.$refs.form).ajaxSubmit({
				url: _this.endpoint,
				dataType: 'json',
				success: function (res) {
					if (res.success) {
						_this.isComplete = true;
						_this.pointsCount = res.points;
						_this.resultId = res.resultId;
						_this.resultTitle = res.resultTitle;
						_this.resultText = res.resultText;
						$(document).trigger('testing.counter.update', {
							id: res.id,
							points: res.points,
							total: res.total
						});
						$('html, body').animate({
							scrollTop: $('#main-stage' + _this.ts).offset().top - $('.header').height()
						}, 500);
					}
				}
			});

			/**
			 * Complete test event
			 */
			console.log('complete-test-' + this.sectionId);
		}
	},
	computed: {
		marginLeft: function () {
			if ($(window).width() <= 768) {
				return this.activeQuestion > 2
					? '-' + ((this.activeQuestion - 2) * 60) + 'px'
					: 0;
			}
		},
		shareUrl: function () {
			return location.origin + '/testresult/' + this.resultId;
		},
		shareUrlFb: function () {
			return 'https://www.facebook.com/sharer.php?u=' + encodeURIComponent(this.shareUrl);
		},
		shareUrlTw: function () {
			return 'http://twitter.com/share?'
				+ 'text=' + encodeURIComponent('Я прошел тест на fincult.info')
				+ '&url=' + encodeURIComponent(this.shareUrl);
		},
		shareUrlVk: function () {
			return 'http://vkontakte.ru/share.php?url=' + encodeURIComponent(this.shareUrl);
		},
		shareUrlOk: function () {
			return 'https://connect.ok.ru/offer?url=' + encodeURIComponent(url)
					+ '&title=' + encodeURIComponent('Я прошел тест на финансовую грамотность.');
		}
	},
	mounted: function () {
		this.getData();
	},
	template: [
		'<section class="questionnaire" :id="\'scroll-to-here\' + ts">\n',
		'	<form class="testing questionnaire__wrap row" :action="endpoint" method="post" enctype="application/x-www-form-urlencoded" ref="form" @submit.prevent="processForm" data-questions-count="questions.length" v-cloak>\n',
		'		<input type="hidden" name="section_id" :value="sectionId"/>\n',
		'		<div class="main-stage" :id="\'main-stage\' + ts" v-show="!isComplete">\n',
		'			<h2 class="questionnaire__title">{{ name }}</h2>\n',
		'			<div class="questionnaire__counter-wrap" id="\'scroll-to-mobile\' + ts">\n',
		'				<ul class="questionnaire__counter" :style="{ \'margin-left\': marginLeft }" v-for="(q, key, index) in questions">\n',
		'					<li class="questionnaire__counter-item" :class="{ _active: activeQuestion == index, _checked: activeQuestion > index }">{{ index + 1 }}</li>\n',
		'				</ul>\n',
		'			</div>\n',
		'			<div v-for="(q, key, index) in questions">\n',
		'				<div class="questionnaire__item" :class="{ _active: activeQuestion == index }">\n',
		'					<h3 class="questionnaire__item-title">{{ q.title }}</h3>\n',
		'					<fieldset class="questionnaire__fieldgroup" v-for="(a, akey) in q.answers">\n',
		'						<input :id="ts + \'option-\' + q.id + \'-\' + akey" type="radio" :name="\'answers[\' + q.id + \']\'" :value="akey"/>\n',
		'						<label :for="ts + \'option-\' + q.id + \'-\' + akey">{{ a.title }}</label>\n',
		'					</fieldset>\n',
		'				</div>\n',
		'			</div>\n',
		'			<button type="button" class="btn questionnaire__btn questionnaire__next-btn" @click="nextQuestion" v-show="!isLastQuestion()">далее</button>\n',
		'			<button type="button" class="btn btn--orange-bg questionnaire__btn questionnaire__prev-btn" v-show="!isFirstQuestion()" @click="prevQuestion" :class="{ disabled: isFirstQuestion() }">назад</button>\n',
		'			<button type="button" class="btn btn--orange-bg questionnaire__btn questionnaire__final-btn" v-show="isLastQuestion()" @click="processForm">Завершить</button>\n',
		'		</div>\n',
		'		<div class="complete-stage" v-show="isComplete">\n',
		'			<div class="alert alert-success" role="alert">\n',
		'				<div class="questionnaire__item questionnaire__item--final">\n',
		'					<h3 class="questionnaire__title">{{ name }}\n',
		'						<div class="articles__test-counter test-complete">\n',
		'							<div class="complete-section"><i class="icon-test-count icon-test-count--black"></i>{{ pointsCount }}/{{ questionsCount }} БАЛЛОВ</div>\n',
		'						</div>\n',
		'					</h3>\n',
		'					<ul class="questionnaire__counter" v-for="(q, key, index) in questions">\n',
		'						<li class="questionnaire__counter-item _checked">{{ index + 1 }}</li>\n',
		'					</ul>\n',
		'					<h4 class="questionnaire__second-title">{{ resultTitle }}</h4>\n',
		'					<p class="questionnaire__final-desc">{{ resultText }}</p>\n',
		'					<div class="social">\n',
		'						<ul class="social__list">\n',
		'							<li class="fb-color">\n',
		'								<a :href="shareUrlFb" :data-link="shareUrlFb"><i class="icon-fb"></i></a>\n',
		'							</li>\n',
		'							<li class="tw-color">\n',
		'								<a :href="shareUrlTw" :data-link="shareUrlTw"><i class="icon-tw"></i></a>\n',
		'							</li>\n',
		'							<li class="vk-color">\n',
		'								<a :href="shareUrlVk" :data-link="shareUrlVk"><i class="icon-vk"></i></a>\n',
		'							</li>\n',
		'							<li class="ok-color">\n',
		'								<a :href="shareUrlOk" :data-link="shareUrlOk"><i class="icon-ok"></i></a>\n',
		'							</li>\n',
		'						</ul>\n',
		'					</div>\n',
		'				</div>\n',
		'			</div>\n',
		'		</div>\n',
		'	</form>\n',
		'	<br clear="all">\n',
		'</section>'].join('')
});


/* End */
;
; /* Start:"a:4:{s:4:"full";s:64:"/local/templates/index/js/customelement_e-test.js?15601600237411";s:6:"source";s:49:"/local/templates/index/js/customelement_e-test.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
Vue.customElement('e-test', {
	props: ['code'],
	data: function () {
		return {
			endpoint: '',
			sectionId: 0,
			name: '',
			description: '',
			questions: {},
			questionsIds: [],
			questionsList: [],
			userAnswers: {},
			currentQuestionIndex: 0,
			isComplete: false,
			isQuestionAnswered: false,
			resultInfo: {},
			shareUrl: {
				fb: '',
				tw: '',
				vk: '',
				ok: ''
			},
			//
			ts: Date.now()
		}
	},
	methods: {
		getData: function () {
			var _this = this;
			$.ajax({
				url: '/local/ajax/testing.php',
				dataType: 'json',
				data: {
					action: 'json',
					section_id: _this.code
				},
				method: 'post',
				success: function (res) {
					_this.endpoint = res.ENDPOINT;
					_this.name = res.NAME;
					_this.questions = res.QUESTIONS;
					_this.questionsIds = res.QUESTIONS_IDS;
					_this.sectionId = res.SECTION_ID;
					_this.description = res.DESCRIPTION;
					//
					res.QUESTIONS_IDS.forEach(function (id) {
						_this.questionsList.push(res.QUESTIONS[id]);
					})
				}
			});
		},
		resetForm: function () {
			this.isComplete = false;
			this.isQuestionAnswered = false;
			this.currentQuestionIndex = 0;
			this.userAnswers = {};

			for (var prop in this.questions) {
				if (this.questions.hasOwnProperty(prop)) {
					this.questions[prop].answers.forEach(function (answer) {
						delete(answer.isCorrect);
					});
				}
			}
		},
		nextQuestion: function () {
			for (var i = 0; i < this.$refs.answers.length; i++) {
				this.$refs.answers[i].checked = false;
			}
			this.isQuestionAnswered = false;
			this.currentQuestionIndex++;
			if (this.currentQuestionIndex == 1) {
				/**
				 * First answer test event
				 */
				yaCounter45637635.reachGoal('first-answer-test-' + this.sectionId);
			}
			if (this.currentQuestionIndex >= this.questionsCount) {
				this.complete();
			}
		},
		checkAnswer: function (answerIndex) {
			var answer = this.currentQuestion.answers[answerIndex];
			this.isQuestionAnswered = true;
			Vue.set(answer, 'isCorrect', answer.points > 0);

			this.currentQuestion.answers.forEach(function (answer) {
				if (answer.points > 0) {
					answer.isCorrect = true;
				}
			});

			this.userAnswers[this.currentQuestion.id] = answerIndex;
		},
		complete: function () {
			var _this = this;
			$.ajax({
				method: 'post',
				url: _this.endpoint,
				data: {
					section_id: _this.sectionId,
					answers: _this.userAnswers
				},
				success: function (res) {
					if (!res.success) {
						alert('Ошибка: ' + res.message);
					}
					_this.resultInfo = res;
					_this.buildShareUrl();
					_this.isComplete = true;

					// обновляем значения на кнопке "Пройти тест еще раз"
					$(document).trigger('testing.counter.update', {
						id: res.id,
						points: res.points,
						total: res.total
					});
				}
			});

			/**
			 * Complete test event
			 */
			yaCounter45637635.reachGoal('complete-test-' + this.sectionId);
		},
		buildShareUrl: function () {
			var url = window.location.origin + '/testresult/' + this.resultInfo.resultId;
			this.shareUrl.fb = 'https://www.facebook.com/sharer.php?u=' + encodeURIComponent(url);
			this.shareUrl.tw = 'https://twitter.com/share?'
				+ 'text=' + encodeURIComponent('Я прошел тест на fincult.info')
				+ '&url=' + encodeURIComponent(url);
			this.shareUrl.vk = 'https://vkontakte.ru/share.php?url=' + encodeURIComponent(url);
			this.shareUrl.ok = 'https://connect.ok.ru/offer?url=' + encodeURIComponent(url)
					+ '&title=' + encodeURIComponent('Я прошел тест на финансовую грамотность.');
		}
	},
	computed: {
		pagination: function () {
			var result = [],
				classNames = [],
				i, start, end,
				pagesCount = this.questionsCount,
				active = this.currentQuestionIndex + 1,
				showPagesCount = 4,
				left = active - 1;

			start = active;
			end = start + showPagesCount - 1;
			if (end > pagesCount) {
				start -= (end - pagesCount);
				end = pagesCount;
				if (start < 1) {
					start = 1;
				}
			}
			for (i = start; i < end; i++) {
				result.push({number: i, class: active === i ? '_active' : ''});
			}
			if (end < pagesCount) {
				classNames.push('_space');
			}
			if (active === end) {
				classNames.push('_active');
			}
			result.push({number: this.questionsCount, class: classNames.join(' ')});

			return result;
		},
		currentQuestion: function () {
			return this.questionsList[this.currentQuestionIndex];
		},
		questionsCount: function () {
			return this.questionsList.length;
		}
	},
	mounted: function () {
		this.getData();
	},
	template: [
		'<section class="exam" id="scroll-to-here">\n',
		'	<div class="exam__form">\n',
		'		<div class="exam__process" v-if="!isComplete">\n',
		'			<h2 class="exam__title">{{ name }}</h2>\n',
		'			<div>{{ description }}</div>\n',
		'			<div class="exam__steps">\n',
		'				<div class="exam__steps-item" v-for="page in pagination" :class="page.class">{{ page.number }}</div>\n',
		'			</div>\n',
		'			<div class="exam__content" v-if="currentQuestion">\n',
		'				<div class="exam__question">{{ currentQuestion.title }}</div>\n',
		'				<div class="exam__answers" :class="{ \'_answered\': isQuestionAnswered }">\n',
		'					<div class="exam__answers-item" v-for="(answer, i) in currentQuestion.answers" >\n',
		'						<input class="exam__answers-input" type="radio" :name="currentQuestion.id + \'_\' + ts" value="0" :class="{ \'_correct\': answer.isCorrect === true, \'_incorrect\': answer.isCorrect === false }" :id="currentQuestion.id + \'_\' + ts + \'option-\' + i" :disabled="isQuestionAnswered" @change="checkAnswer(i)" ref="answers">\n',
		'						<label class="exam__answers-label" :for="currentQuestion.id + \'_\' + ts + \'option-\' + i"><span><i class="exam__icon"></i>{{ answer.title }}</span></label>\n',
		'						<div class="exam__answers-desc" v-if="isQuestionAnswered && answer.comment">{{ answer.comment }}</div>\n',
		'					</div>\n',
		'				</div>\n',
		'				<button type="button" class="btn btn--orange-bg exam__btn" @click.prevent="nextQuestion" v-if="isQuestionAnswered">Далее</button>\n',
		'			</div>\n',
		'		</div>\n',
		'		<div class="exam__complete" v-if="isComplete">\n',
		'			<h2 class="exam__title">{{ name }}</h2>\n',
		'			<div class="exam__complete-result"><i class="icon-test-count icon-test-count--black exam__complete-result-icon"></i>{{ resultInfo.points }}/{{ resultInfo.total}} Баллов</div>\n',
		'			<h4 class="exam__complete-title">{{ resultInfo.resultTitle }}</h4>\n',
		'			<p class="exam__complete-desc">{{ resultInfo.resultText }}</p>\n',
		'			<p class="exam__complete-reset"><a class="btn btn--orange-bg" href="#" @click.prevent="resetForm">Пройти ещё раз</a></p>\n',
		'			<div class="social">\n',
		'				<ul class="social__list">\n',
		'					<li class="fb-color"><a href="#" :href="shareUrl.fb" :data-link="shareUrl.fb"><i class="icon-fb"></i></a></li>\n',
		'					<li class="tw-color"><a href="#" :href="shareUrl.tw" :data-link="shareUrl.tw"><i class="icon-tw"></i></a></li>\n',
		'					<li class="vk-color"><a href="#" :href="shareUrl.vk" :data-link="shareUrl.vk"><i class="icon-vk"></i></a></li>\n',
		'					<li class="ok-color"><a href="#" :href="shareUrl.ok" :data-link="shareUrl.ok"><i class="icon-ok"></i></a></li>\n',
		'				</ul>\n',
		'			</div>\n',
		'		</div>\n',
		'	</div>\n',
		'</section>'].join('')
});


/* End */
;
; /* Start:"a:4:{s:4:"full";s:64:"/local/templates/index/js/customelement_v-vote.js?15601600237560";s:6:"source";s:49:"/local/templates/index/js/customelement_v-vote.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
Vue.customElement('v-vote', {
	props: ['id'],
	data: function () {
		return {
			// data
			endpoint: '/local/ajax/vote.php',
			title: undefined,
			sessionId: undefined,
			captcha: undefined,
			questions: undefined,
			// status
			status: undefined,
			success: false,
			message: undefined,
			// pagination
			currentQuestionIndex: 0,
			// state
			fieldState: {}
		}
	},
	methods: {
		getData: function () {
			var _this = this;
			$.ajax({
				url: _this.endpoint,
				dataType: 'json',
				data: {
					VOTE_ID: _this.id,
				},
				method: 'get',
				success: function (res) {
					if (res.status) {
						_this.status = true;
						_this.title = res.TITLE;
						_this.sessionId = res.SESSION_ID;
						_this.questions = res.QUESTIONS;
					} else {
						_this.status = false;
					}
					_this.message = res.message;
				}
			});
		},
		processForm: function () {
			var _this = this;
			$(this.$refs.form).ajaxSubmit({
				url: _this.endpoint,
				dataType: 'json',
				success: function (res) {
					if (res.status) {
						_this.success = true;
					} else {
						_this.status = false;
					}
					_this.message = res.message;
				}
			});
		},
		nextQuestion: function () {
			this.currentQuestionIndex++;
		},
		prevQuestion: function () {
			this.currentQuestionIndex--;
		},
		isFirstQuestion: function () {
			return this.currentQuestionIndex == 0;
		},
		isLastQuestion: function () {
			return this.currentQuestionIndex == (this.questionsCount - 1);
		},
		isChecked: function (qid, value) {
			return this.fieldState[qid] == value;
		},
		setState: function (qid, value) {
			Vue.set(this.fieldState, qid, value);
		}
	},
	computed: {
		pagination: function () {
			var result = [],
				classNames = [],
				i, start, end,
				pagesCount = this.questionsCount,
				active = this.currentQuestionIndex + 1,
				showPagesCount = 4,
				left = active - 1;

			start = active;
			end = start + showPagesCount - 1;
			if (end > pagesCount) {
				start -= (end - pagesCount);
				end = pagesCount;
				if (start < 1) {
					start = 1;
				}
			}
			for (i = start; i < end; i++) {
				result.push({number: i, class: active === i ? '_active' : ''});
			}
			if (end < pagesCount) {
				classNames.push('_space');
			}
			if (active === end) {
				classNames.push('_active');
			}
			result.push({number: this.questionsCount, class: classNames.join(' ')});

			return result;
		},
		currentQuestion: function () {
			return this.questions[this.currentQuestionIndex];
		},
		questionsCount: function () {
			return this.questions.length;
		}
	},
	mounted: function () {
		this.getData();
	},
	template: [
		'<section class="vote">\n',
		'	<div v-if="status" class="vote__form">\n',
		'		<h3 class="vote__title">{{ title }}</h3>\n',
		'		<div class="exam__steps">\n',
		'			<div class="exam__steps-item" v-for="page in pagination" :class="page.class">{{ page.number }}</div>\n',
		'		</div>\n',
		'		<form :action="endpoint" method="post" class="vote-form" ref="form" @submit.prevent="processForm">\n',
		'			<input type="hidden" name="vote" value="Y">\n',
		'			<input type="hidden" name="PUBLIC_VOTE_ID" :value="id">\n',
		'			<input type="hidden" name="VOTE_ID" :value="id">\n',
		'			<input type="hidden" name="sessid" :value="sessionId">\n',
		'			<ol class="vote-items-list vote-question-list vote__answers">\n',
		'				<li v-for="(q, key, index) in questions" class="vote-item-vote vote__answers-item" v-show="key == currentQuestionIndex">\n',
		'					<div class="vote-item-header">\n',
		'						<div v-if="q.IMAGE" class="vote-item-image"><img :src="q.IMAGE.SRC" width="30" height="30" /></div>\n',
		'						<div class="vote-item-title vote-item-question vote__question">{{ q.QUESTION }}<span v-if="q.REQUIRED" class="starrequired">*</span></div>\n',
		'						<div class="vote-clear-float"></div>\n',
		'					</div>\n',
		'					<ol class="vote-items-list vote-answers-list vote__answers">\n',
		'						<li v-for="a in q.ANSWERS" class="vote-item-vote vote__answers-item">\n',
		'							<span v-if="a.FIELD_TYPE == 0" class="vote-answer-item vote-answer-item-radio">\n',
		'								<input type="radio" class="vote__answers-input" @change="setState(a.QUESTION_ID, a.ID)" :value="a.ID" :name="\'vote_radio_\' + a.QUESTION_ID" :id="\'vote_radio_\' + a.ID"/>\n',
		'								<label :for="\'vote_radio_\' + a.ID"  class="vote__answers-label vote__answers-label_radio">{{ a.MESSAGE }}</label>\n',
		'							</span>\n',
		'							<span v-if="a.FIELD_TYPE == 1" class="vote-answer-item vote-answer-item-checkbox">\n',
		'								<input type="checkbox" class="vote__answers-input" :name="\'vote_checkbox_\' + a.QUESTION_ID + \'[]\'" :value="a.ID" :id="\'vote_checkbox_\' + a.ID" />\n',
		'								<label :for="\'vote_checkbox_\' + a.ID" class="vote__answers-label vote__answers-label_checkbox">{{ a.MESSAGE }}</label>\n',
		'							</span>\n',
		'							<span v-if="a.FIELD_TYPE == 2" class="vote-answer-item vote-answer-item-dropdown">\n',
		'								<select :name="\'vote_dropdown_\' + a.QUESTION_ID">\n',
		'									<option v-for="dd in a.DROPDOWN" :value="dd.ID">{{ dd.MESSAGE }}</option>\n',
		'								</select>\n',
		'							</span>\n',
		'							<span v-if="a.FIELD_TYPE == 3" class="vote-answer-item vote-answer-item-multiselect">\n',
		'								<select :name="\'vote_multiselect_\' + a.QUESTION_ID + \'[]\'" multiple="multiple">\n',
		'									<option v-for="ms in a.MULTISELECT" :value="ms.ID">{{ ms.MESSAGE }}</option>\n',
		'								</select>\n',
		'							</span>\n',
		'							<span v-if="a.FIELD_TYPE == 4"  class="vote-answer-item vote-answer-item-textfield">\n',
		'								<input type="radio" class="vote__answers-input" @change="setState(a.QUESTION_ID, a.ID)" :value="a.ID" :name="\'vote_radio_\' + a.QUESTION_ID" :id="\'vote_radio_\' + a.ID"/>\n',
		'								<label :for="\'vote_radio_\' + a.ID"  class="vote__answers-label vote__answers-label_radio _textfield">\n',
