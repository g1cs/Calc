; /* Start:"a:4:{s:4:"full";s:50:"/local/templates/index/js/common.js?15601600232337";s:6:"source";s:35:"/local/templates/index/js/common.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(function() {
	"use strict";

	var lock = false,
		regForm = $('#register_form');

	// register

	regForm.validate({
		rules: {
			'REGISTER[EMAIL]': {
				email: true,
				required: true
			},
			'captcha_word': {
				required: true
			}
		},
		submitHandler: function() {
			if (lock) {
				return false;
			}
			lock = true;
			$.post('/local/ajax/register.php', regForm.serialize(), function(response) {
				if (response.status != 'ok') {
					regForm.find('.form-error')
						.html(response.message)
						.show();
					return false;
				}
				regForm.validate().destroy();
				regForm.closest('.pop-up__wrap').addClass('pop-up__wrap--success').html('<a href="#" class="pop-up__close popup-modal-dismiss"></a>' + response.message);
			}).always(function() {
				$(document).trigger('captcha_renew');
				lock = false;
			});

			return false;
		}
	});

	$('.show-reg').on('click', function() {
		$(document).trigger('captcha_renew');
	});

	// document events
	$('#status').fadeOut();
	$('#preloader').delay(350).fadeOut('slow');
	$('.captcha-block').initCaptcha();
});


(function($) {
	$.fn.initCaptcha = function() {
		this.each(function() {

			var _this = $(this),
				lock = false,
				renew = _this.find('.captcha-renew'),
				input = _this.find('input[name=captcha_sid]'),
				word = _this.find('input[name=captcha_word]'),
				image = _this.find('.captcha-image'),
				updateImage = function() {
					if (lock) {
						return false;
					}
					lock = true;
					$.post('/local/ajax/captcha.php', {}, function(data) {
						if (!data) {
							alert('server error');
						}
						image.attr('src', '/bitrix/tools/captcha.php?captcha_sid=' + data.code);
						input.val(data.code);
						word.val('');
					}, 'json').always(function() {
						lock = false;
					});
					return false;
				};

			if (_this.data('captcha_pass')) {
				return;
			}
			_this.data('captcha_pass', 1);

			renew.on('click', updateImage);
			image.on('click', updateImage);
			$(document).on('captcha_renew', updateImage);

		});
		return this;
	};

	$.scrollToTarget = function (target) {
		var $target = $(target);

		if ($target.length) {
			var $header = $('.header__bot'),
				offset = $header.length ? $header.outerHeight() : 0;

			$('html, body').animate({
				scrollTop: $target.offset().top - offset
			}, 1000);
		}
	}
})(jQuery);

/* End */
