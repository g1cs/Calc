; /* Start:"a:4:{s:4:"full";s:48:"/local/templates/index/js/form.js?15601600231138";s:6:"source";s:33:"/local/templates/index/js/form.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
  (function() {

	  var inputText1 = function(element, nameAttr) {
		  $("" + element + "[name*='" + nameAttr + "']").val('');
	  };

	  var inputText2 = function(element, nameAttr) {
		  $("" + element + "[name*='" + nameAttr + "']").val('');
	  };

	  var inputText3 = function(element, nameAttr) {
		  $("" + element + "[name*='" + nameAttr + "']").val('');
	  };

	  function Cleaner() {
		  this.subscribers = [];
	  }

	  Cleaner.prototype.clear = function(data1, data2) {
		  this.subscribers.forEach(
			  function(fn) {
				  fn(data1, data2);
			  }
		  );
		  return this;
	  };

	  Function.prototype.subscribe = function(publisher) {
		  publisher.subscribers.push(this);
		  return this;
	  };

	  var cleanField = new Cleaner;

	  inputText1.subscribe(cleanField);
	  inputText2.subscribe(cleanField);
	  inputText3.subscribe(cleanField);


	  $("input[type*='reset']").on("click", function(e) {
		  e.preventDefault();
		  cleanField.clear('input', 'form_text_1');
		  cleanField.clear('input', 'form_email_2');
		  cleanField.clear('textarea', 'form_textarea_3');
		  $(location).attr("href", "/feedback");
	  })

  })()

/* End */
