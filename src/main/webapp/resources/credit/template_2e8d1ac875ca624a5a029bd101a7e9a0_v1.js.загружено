; /* Start:"a:4:{s:4:"full";s:52:"/local/templates/index/js/pluralize.js?1560160023271";s:6:"source";s:38:"/local/templates/index/js/pluralize.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
var pluralize = function (number, one, two, five) {
	number = Math.abs(number);
	number %= 100;
	if (number >= 5 && number <= 20) {
		return five;
	}
	number %= 10;
	if (number === 1) {
		return one;
	}
	if (number >= 2 && number <= 4) {
		return two;
	}
	return five;
};
/* End */
;
; /* Start:"a:4:{s:4:"full";s:58:"/local/templates/index/js/filter-pluralize.js?156016002336";s:6:"source";s:45:"/local/templates/index/js/filter-pluralize.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
Vue.filter('pluralize', pluralize);

/* End */
