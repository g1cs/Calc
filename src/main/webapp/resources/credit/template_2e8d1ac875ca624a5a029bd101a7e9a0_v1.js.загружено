; /* Start:"a:4:{s:4:"full";s:83:"/local/components/dalee/register.teacher/templates/.default/script.js?1560160023589";s:6:"source";s:69:"/local/components/dalee/register.teacher/templates/.default/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(function () {

	var form = $('#auth-form');

	form.ajaxForm({
		datatType: 'json',
		status: '',
		success: function (res) {
			$(document).trigger('captcha_renew');

			$(".pop-up__error").remove();

			if (res.status != 'ok') {
				$('.pop-up__reg-teach').find('.pop-up__notification')
					.before("<p class='pop-up__error'>" + res.message + "</p>");

				return false;
			}

			form.closest('.pop-up__wrap').addClass('pop-up__wrap--success').html('<a href="#" class="pop-up__close popup-modal-dismiss"></a>' + res.message);
		}
	});

	$(".field--number").mask("(999) 999-9999");

});
/* End */
