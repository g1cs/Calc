; /* Start:"a:4:{s:4:"full";s:49:"/local/templates/index/js/table.js?15601600231947";s:6:"source";s:34:"/local/templates/index/js/table.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
(function(){
	'use strict';
	var tableClassName = [
		{
			'table': 'table', //Класс Таблицы
			'tableWrap': 'table__wrap-for-after', //Класс обертки таблицы
			'tableAfter': 'table__wrap-for-after--show' //Класс градиента таблицы при overflow
		},
		{
			'table': 'calc-table',
			'tableWrap': 'calc-result__table-wrap',
			'tableAfter': 'calc-result__table-wrap--after'
		},
	];

	var showTableOverlay = function(table) {
		for (var i = 0; i < table.length; i++) {
			var wrapWidth = $('.' + table[i].tableWrap).width();
			$('.' + table[i].table).each(function () {
				if ($(this).width() > wrapWidth) {
					$(this).closest('.' + table[i].tableWrap).addClass(' ' + table[i].tableAfter);
				} else {
					$(this).closest('.' + table[i].tableWrap).removeClass(' ' +table[i].tableAfter);
				}
			});
		}
	};

	if( $('.table, .calc-table').length > 0 ) {
		$(window).resize(function() {
			if( $(window).width() > 768 ) {
				showTableOverlay(tableClassName);
			} else if ( $(window).width() < 768 ) {
				showTableOverlay(tableClassName);
			}
		});
	}

	var shadowRow;
	$('.table td').on('mouseover', function() {
		$(this).addClass('top-shadow').addClass('bottom-shadow');
		if ($(this).is('[rowspan]')) {
			var row_count = parseInt($(this).attr('rowspan'));
			$(this).siblings('td').addClass('top-shadow');
			shadowRow = $(this).closest('tr').nextAll().andSelf().slice(row_count-1, row_count);
			shadowRow.find('td').addClass('bottom-shadow')
		} else {
			var sibling_td = $(this).siblings('td');
			$(sibling_td).each(function() {
				if (!$(this).is('[rowspan]')) {
					$(this).addClass('top-shadow').addClass('bottom-shadow')
				}
			})
		}
	})
	.on('mouseout', function() {
		$(this).removeClass('top-shadow').removeClass('bottom-shadow').siblings('td').removeClass('top-shadow').removeClass('bottom-shadow')
		shadowRow.find('td').removeClass('bottom-shadow');
	});

})()

/* End */
;
; /* Start:"a:4:{s:4:"full";s:51:"/local/templates/index/js/td-align.js?1560160023429";s:6:"source";s:37:"/local/templates/index/js/td-align.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
(function() {
	$(document).ready(function() {
		var table = $('.table');

		if (table.length) {
			table.find('td').each(function(index, el) {
				var td = $(el);
				var tdText = td.text().replace(/\s/g, '');

				if (!tdText.length) return;

				if (el.style.textAlign.length > 0) return;

				if (isNaN(tdText)) {
					td.css('text-align', 'left');
				} else {
					td.css('text-align', 'right');
				}
			});
		}
	});
})();

/* End */
;
; /* Start:"a:4:{s:4:"full";s:48:"/local/templates/index/js/like.js?15601600231277";s:6:"source";s:33:"/local/templates/index/js/like.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
(function() {
	'use strict';

	function removeStyleAttr() {
		$('.liked__item').map(function(i, val) {
			$(val).removeAttr('style');
		})
	}

	function hideBottomBorderToItem(num, width) {
		var items = $('.liked__item');
		
		$.each(items, function(i, val) {
			if( findLastItem.lengthArr - num === i + 1) {
				if(  width > 1024) {
					$( 'div.liked__item:nth-child(1n+' + (i + 2)).css('border-bottom', '0');
					return
				}
				if( width < 1024 ) {
					$( 'div.liked__item:nth-child(1n+' + (i + 3)).css('border-bottom', '0');
				}
			}
		})

	}

	function findLastItem(cnt, width) {
		if( $('.liked').length > 0 ) {
			var items = $('.liked__item');
			findLastItem.lengthArr = $('.liked__item').length;

			switch(findLastItem.lengthArr % cnt) {
				case 1: hideBottomBorderToItem( 1, width)
					break;
				case 2: hideBottomBorderToItem( 2, width )
					break;
				case 0: hideBottomBorderToItem( 3, width )
					break;
			}
		}
	}

	$(window).resize(function() {
		removeStyleAttr();
		if( $(this).width() > 1024 ) {
			findLastItem(3, $(this).width() );
			return;
		} else if ( $(this).width() <= 767 ) {
			removeStyleAttr();
			return;
		} else if ( $(this).width() <= 1024 ) {
			findLastItem(2, $(this).width() );
		}
	})

	$(window).trigger('resize');
})()

/* End */
;
; /* Start:"a:4:{s:4:"full";s:57:"/local/templates/index/js/event-calendar.js?1560160023327";s:6:"source";s:43:"/local/templates/index/js/event-calendar.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(document).ready(function() {
	var eventsPageHeading = $('.events-page').find('.section__wrap h1');
	eventsPageHeading.append('<button class="event-calendar__toggler">Календарь</button>');

	$('.event-calendar__toggler').on('click', function () {
		$('.event-calendar').toggleClass('event-calendar_active');
	});
});

/* End */
