; /* Start:"a:4:{s:4:"full";s:48:"/local/templates/index/scripts.js?15601600238076";s:6:"source";s:33:"/local/templates/index/scripts.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
var _rutarget = window._rutarget || [];

(function ($) {
    'use strict';

    var passTestBtn = $('.js-pass-test-btn');
    passTestBtn.on('click', function() {
        _rutarget.push({'event': 'thankYou', 'conv_id': 'test'});
    });

    $(document).ready(function() {
        initMobileOpenTarget();
        initBlindMode();
        showMore();
        showMoreBtnInit();

        if ($(window).innerWidth() > 1024) {
            dotListItem();
        }

        if ($('.knowledge__nav').length) {
            initKnowledgeBaseScrollingMobile();
        }

        var host = window.location.hostname;
        $('a[href^="http"]:not([target="_blank"])').each(function() {
            var url = $(this).attr('href');
            var a = $('<a>', { href: url});
            if (host!== a.prop('hostname')) {
                $(this).attr('target', '_blank');
            }
        });
    });

    $(window).resize(function() {
        showMoreBtnInit();
    });

    var showMoreBtnInit = function () {
        var listItemHeight = $(window).innerWidth() < 768 ? 85 : 110;
        var $catalogFilter = $('.catalog-filter__nav-list');
        if ($catalogFilter.length) {
            var catalogNavListHeight = $catalogFilter[0].scrollHeight;
            if (catalogNavListHeight <= listItemHeight) {
                $('.js-show-more').addClass('active');
            } else {
                $('.js-show-more').removeClass('active');
            }
        }
    };

    var initMobileOpenTarget = function() {
        $(document).on('click', '.js-open-target', function() {
            if ($(window).innerWidth() < 768) {
                var targetClass = '.' + $(this).data('target');
                $(targetClass).toggle();
            }
        })
    };

    var initKnowledgeBaseScrollingMobile = function() {

        var widthAllItems = 0,
            $nav = $('.knowledge__nav'),
            $item = $('.knowledge__nav-item'),
            $itemNotBound = $('.knowledge__nav-item:not(.bound)'),
            $wrapper = $('.knowledge__nav-wrapper');

        function setWidth() {
            widthAllItems = 0;
            $item.each(function() {
                $('.knowledge__nav-wrapper').css('width','auto');
                $(this).css('width','auto');
                $(this).innerWidth($(this).innerWidth());
                widthAllItems += $(this).innerWidth();
            });
            $wrapper.width(widthAllItems + $(window).innerWidth() - $item.last().innerWidth());
        }

        function animateScrollLeft() {
            var left = $(this).position().left;
            $nav.animate({
                scrollLeft: left
            }, 'fast');
        }

        if ($(window).innerWidth() < 1024) {
            setWidth();
            $itemNotBound.addClass('bound').on('click', animateScrollLeft);
        }

        $(window).resize(function() {
            if ($(window).innerWidth() < 1024) {
                setWidth();
                $('.knowledge__nav-item:not(.bound)').addClass('bound').on('click', animateScrollLeft);
                var left = $('.knowledge__nav-item._active').position().left;
                $nav.scrollLeft(left);
            } else {
                $wrapper.css('width','');
                $item.css('width','');
                $item.removeClass('bound').off('click', animateScrollLeft);
            }
        });

    };

    $(document).on('click', '.reestr-item__title', toggleReestrItem);
    $(document).on('change', '.reestr-sections__select', selectReestrSection);
    $(document).on('click', '.js-tab', initTabs);

    // tabs
    function initTabs() {
        var $self = $(this),
            $tab = $self.data('tab');

        $('[data-tab-content]').removeClass('_active');
        $('.js-tab').removeClass('_active');

        $('[data-tab-content="' + $tab + '"]').addClass('_active');
        $self.addClass('_active');
    }


    function toggleReestrItem() {
        $('.reestr-item__body_show').removeClass('reestr-item__body_show');
        $(this).parents('.reestr-item').find('.reestr-item__body').toggleClass('reestr-item__body_show');
    }

    function selectReestrSection() {
        window.location.href = $(this).val();
    }

    var detectIE = function () {
            var ua = window.navigator.userAgent;

            var msie = ua.indexOf('MSIE ');
            if (msie > 0) {
                return parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
            }

            var trident = ua.indexOf('Trident/');
            if (trident > 0) {
                var rv = ua.indexOf('rv:');
                return parseInt(ua.substring(rv + 3, ua.indexOf('.', rv)), 10);
            }

            var edge = ua.indexOf('Edge/');
            if (edge > 0) {
                return parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10);
            }

            return false;
        },
        isIE = detectIE();

    if (isIE) {
        $('body').addClass('_ie _ie' + isIE);
    }

    var accordions = $('.js-accordion');
    if (accordions.length) {
        accordions.accordion({
            collapsible: true,
            active: false,
            heightStyle: 'content',
            icons: false
        });
    }

    var showMore = function() {

        $(document).on('click', '.js-show-more', function() {

            var target = $(this).data('content-class');

            !$(this).siblings('.' + target).hasClass('_open') ? $(this).addClass('active').siblings('.' + target).addClass('_open') : $(this).removeClass('active').siblings('.' + target).removeClass('_open');

            if ($(this).data('hide')) {
                $(this).hide();
            }
        })
    };

    var initBlindMode = function() {
        var blindBtn = $('.js-blind-mode');

        blindBtn.on('click', function() {
            $('html').toggleClass('blind');

            var blinded = $('html').hasClass('blind');
            $.cookie('blind', blinded ? 1 : 0, {path: '/'});
        })
    };

    // jquery validate custom rules
    $.validator.addMethod('emailBitrix', function (value, element) {
        var email = $.trim(value);
        var result = email.match(/^[=_0-9a-z+~'!$&*^`|\#%\/?{}-]+(\.[=_0-9a-z+~'!$&*^`|\#%\/?{}-]+)*@(([-0-9a-z_]+\.)+)([a-z0-9-]{2,20})$/i);
        return !!result;
    }, 'Введите корректный адрес электронной почты.');

    $.validator.addMethod('password_check_upper', function (value) {
        return /[A-Z]/.test(value);
    });

    $.validator.addMethod('password_check_lower', function (value) {
        return /[a-z]/.test(value);
    });

    $.validator.addMethod('password_check_digits', function (value) {
        return /[0-9]/.test(value);
    });

    $.validator.addMethod('password_check_special', function (value) {
        return /[,\.\<\>/\?;\:'"\[\]\{\}\|`~\!@#\$%\^&\*\(\)\-_\+\=]/.test(value);
    });

    $.validator.addMethod('password_check_cyrillic', function (value) {
        return !/[а-яА-ЯёЁ]/.test(value);
    });

    $.validator.addMethod('only_spaces_check', function (value) {
        return /\S/.test(value);
    }, 'Значение не может состоять из пробелов');

    var dotListItem = function() {
        var listItemInfo  = $('.list__item-info'),
            listItemFeature = $('.list__item-info-feature');

        var config = {
            ellipsis: '...',
            wrap: 'word',
            fallbackToLetter: true,
            tolerance: 4,
            watch: true,
            lastCharacter: {
                remove: [' ', ',', ';', '.', '!', '?'],
                noEllipsis: []
            }
        };

        listItemFeature.find('b').dotdotdot(config);

        listItemInfo.on('mouseenter', function() {
            $(this).find('.list__item-info-feature b').trigger('destroy.dot');
        });

        listItemInfo.on('mouseleave', function() {
            $(this).find('.list__item-info-feature b').dotdotdot(config);
        });
    };
})(jQuery);

Vue.config.productionTip = false;

/* End */
