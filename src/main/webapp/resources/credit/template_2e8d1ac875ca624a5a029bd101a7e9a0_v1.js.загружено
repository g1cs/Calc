(function() {

	var responsive = {
		tablet: 768,
		lowDesktop: 1120
	};

	$(document).ready(function() {
		var promoSwiper;
		var promoSwiperSettings = {
			pagination: '.promo__pagination',
			paginationClickable: true,
			spaceBetween: 50,
			bulletActiveClass: "_active",
			loop: true,
			bulletClass: "promo__pagination-bullet",
			nextButton: ".promo__button-next",
			prevButton: ".promo__button-prev"
		};
		if ($(".promo").length) {
			promoSwiper = new Swiper('.promo', promoSwiperSettings);
		}

		var aboutSwiperSettings = {
			slidesPerView: 4,
			paginationClickable: true,
			spaceBetween: 30,
			autoHeight: true,
			breakpoints: {
				320: {
					slidesPerView: 2,
					spaceBetween: 10
				},
				480: {
					slidesPerView: 2,
					spaceBetween: 20
				},
				640: {
					slidesPerView: 3,
					spaceBetween: 20
				},
				768: {
					slidesPerView: 4,
					spaceBetween: 30
				},
				1024: {
					slidesPerView: 5,
					simulateTouch: true
				},
				1920: {
					slidesPerView: 6,
					simulateTouch: false
				}
			}
		};



		var aboutSwiper;
		if ($(".about__wrap").length) {
			aboutSwiper = new Swiper('.about__wrap', aboutSwiperSettings);
		}


		var categorySwiper;
		var categorySwiperSettings = {
			slidesPerView: 4,
			paginationClickable: true,
			runCallbacksOnInit: true,
			slideToClickedSlide: true,
			scrollbar: '.swiper-scrollbar',
			scrollbarHide: false,
			onClick: function(categorySwiper) {
				localStorage.currentIndex = categorySwiper.activeIndex;
			},
			onInit: function(categorySwiper) {
				localStorage.currentIndex = $('.category-list__item[data-section_id="' + $(".articles__content-item").attr("class").match(/\d+/)[0] + '"]').attr('data-index');
				categorySwiper.slideTo(localStorage.currentIndex);
			},
			onDestroy: function() {
				return categorySwiper;
			},
			breakpoints: {
				320: {
					slidesPerView: 2,
					spaceBetween: 0
				},
				480: {
					slidesPerView: 2,
					spaceBetween: 0
				},
				640: {
					slidesPerView: 2,
					spaceBetween: 0
				},
				768: {
					slidesPerView: 2,
					spaceBetween: 0

				},
				1024: {
					slidesPerView: 2,
					spaceBetween: 0
				},
				1120: {
					slidesPerView: 3,
					spaceBetween: 0
				}
			}
		};

		if ($(".articles__left").length) {
			if ($(window).width() <= responsive.tablet) {
				categorySwiper = new Swiper('.articles__left', categorySwiperSettings);
			}

			$(window).on("orientationchange", function() {
				if ($(window).width() <= responsive.tablet) {
					if ($(".category-list").length) {
						categorySwiper = new Swiper('.articles__left', categorySwiperSettings);
					}
				} else if ($(window).width() >= responsive.tablet) {
					categorySwiper.destroy(false, true);
				}
			});
		}

		if ($(".js-selfscoring-tabs-controls").length) {
			if ($(window).width() <= responsive.lowDesktop) {
				categorySwiper = new Swiper('.js-selfscoring-tabs-controls', categorySwiperSettings);
			}

			$(window).on("orientationchange", function() {
				if ($(window).width() <= responsive.lowDesktop) {
					if ($(".category-list").length) {
						categorySwiper = new Swiper('.js-selfscoring-tabs-controls', categorySwiperSettings);
					}
				} else if ($(window).width() >= responsive.lowDesktop) {
					categorySwiper.destroy(false, true);
				}
			});
		}



		var contentCarousel;
		var contentCarouselSettings = {
			slidesPerView: 1,
			paginationClickable: true,
			runCallbacksOnInit: true,
			slideToClickedSlide: true,
			loop: true,
			nextButton: ".content-carousel__button-next",
			prevButton: ".content-carousel__button-prev",
			pagination: ".content-carousel__pagination",
			bulletClass: "content-carousel__pagination-bullet",
			bulletActiveClass: "_active"
		};

		if ($(".content-carousel__wrap").length) {
			categorySwiper = new Swiper('.content-carousel__wrap', contentCarouselSettings);
		}
	});

})();

