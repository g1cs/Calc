; /* Start:"a:4:{s:4:"full";s:57:"/local/templates/index/js/header-search.js?15601600231005";s:6:"source";s:42:"/local/templates/index/js/header-search.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
(function () {
	var searchWrap = $('.header__search'),
		menuTop = $('.menu-top'),
		searchInput = $('.header__search-input'),
		showSearchField = function () {
			var searchVisible;

			$('body').on('click', function (e) {
				if ($(e.target).hasClass('header__search-btn') && !searchVisible) {
					e.preventDefault();
					searchWrap.addClass('_active');
					menuTop.addClass('menu-top--width');
					searchVisible = true;
				} else if (!$(e.target).hasClass('header__search-input')) {
					searchWrap.removeClass('_active');
					menuTop.removeClass('menu-top--width');
					searchVisible = false;
				}
			})

		};
	showSearchField();

	var inputData = '';
	searchInput.on('input', function(e) {
		inputData = $(e.target).context.value;
	})

	$('.header__search').find('form').submit(function(e) {
		if( inputData.length === 0 ) {
			e.preventDefault();
			searchWrap.removeClass('_active');
			menuTop.removeClass('menu-top--width');
			searchVisible = false;
		} else {
			return;
		}
	})

})();

/* End */
;
; /* Start:"a:4:{s:4:"full";s:50:"/local/templates/index/js/search.js?15601600231010";s:6:"source";s:35:"/local/templates/index/js/search.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
(function () {
	'use strict';

	if ($('.search-page').length > 0) {

		var addHideStr = function () {
			$('.search-page__category-field').addClass('search-page__category-field--after').attr('data-content', hideStr.str)
		};

		var hideStr = function (str, strCount) {
			hideStr.str = '';
			var strArr = str.split('');
			var newStr = [];
			for (var i = 0; i < strArr.length; i++) {
				newStr.push(strArr[i]);
				if (i > strCount) {
					newStr.push('...');
					newStr = newStr.join();
					hideStr.str = newStr.replace(/,/g, '');
					addHideStr();
					return;
				}
			}
		};

		var blindMode = $('html').hasClass('blind');
		var counter = blindMode ? 18 : 24;

		$('.header__visually').on('click', function () {

			if (!blindMode) {
				counter = 13;
				$(window).trigger('resize');
				$('select.personal__field').trigger('change');
				return;
			}

			counter = 21;
			$(window).trigger('resize');
			$('select.personal__field').trigger('change');
		});

	}

	$(window).trigger('resize');
})();

/* End */
