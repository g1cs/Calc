(function($) {

	$(document).ready(function() {
		stickyHeader();
	});

	$(window).scroll(function() {
		stickyHeader();
	});

	var header = $('.header');

	var stickyHeader = function() {
		if ($(document).scrollTop() > header.height()) {
			header.addClass('scrolled');
		} else {
			header.removeClass('scrolled');
		}
	};
}(jQuery));

