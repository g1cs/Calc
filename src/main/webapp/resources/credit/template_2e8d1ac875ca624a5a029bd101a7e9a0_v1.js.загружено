; /* Start:"a:4:{s:4:"full";s:52:"/local/templates/index/js/calendar.js?15601600234093";s:6:"source";s:37:"/local/templates/index/js/calendar.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
/*global $*/

(function () {
	function isDateCorrect(date) {
		var dateParts = date.split('.'),
		dateObject,
		testerDate = '';
		if (dateParts.length != 3) {
			return false;
		}
		if (/^(((0|1|2){1}\d{1})|(30|31){1})\.(01|02|03|04|05|06|07|08|09|10|11|12){1}\.\d{4}$/.exec(date) == null) {
			return false;
		}
		for (var i in dateParts) {
			if (isNaN(+dateParts[i])) {
				return false;
			} else {
				dateParts[i] = +dateParts[i];
			}
		}
		dateObject = new Date(dateParts[2], dateParts[1] - 1, dateParts[0]);
		testerDate;
		if (dateObject.getDate() < 10) {
			testerDate += '0';
		}
		testerDate += dateObject.getDate().toString();
		testerDate += '.';
		if (dateObject.getMonth() < 9) {
			testerDate += '0';
		}
		testerDate += (dateObject.getMonth() + 1).toString();
		testerDate += '.';
		testerDate += dateObject.getFullYear().toString();
		if (testerDate != date) {
			return false;
		}
		return true;
	}

	var lock = false,
	queryString = window.location.search;

	$.datepicker.regional['ru'] = {
		closeText: 'Закрыть',
		prevText: '&#x3c;Пред',
		nextText: 'След&#x3e;',
		currentText: 'Сегодня',
		monthNames: ['Январь', 'Февраль', 'Март', 'Апрель', 'Май', 'Июнь',
			'Июль', 'Август', 'Сентябрь', 'Октябрь', 'Ноябрь', 'Декабрь'
		],
		monthNamesShort: ['Янв', 'Фев', 'Мар', 'Апр', 'Май', 'Июн',
			'Июл', 'Авг', 'Сен', 'Окт', 'Ноя', 'Дек'
		],
		dayNames: ['воскресенье', 'понедельник', 'вторник', 'среда', 'четверг', 'пятница', 'суббота'],
		dayNamesShort: ['вск', 'пнд', 'втр', 'срд', 'чтв', 'птн', 'сбт'],
		dayNamesMin: ['Вс', 'Пн', 'Вт', 'Ср', 'Чт', 'Пт', 'Сб'],
		weekHeader: 'Не',
		dateFormat: 'dd.mm.yy',
		firstDay: 1,
		isRTL: false,
		showMonthAfterYear: false,
		yearSuffix: ''
	};

	if (queryString) {
		queryString = queryString.substr(1);
		var queryPairs = queryString.split('&'),
		queryPair;
		for (var i in queryPairs) {
			queryPair = queryPairs[i].split('=');
			if (queryPair.length != 2) {
				continue;
			}
			if (queryPair[0] !== 'date') {
				continue;
			}
			if (isDateCorrect(queryPair[1])) {
				$.datepicker.regional['ru']['defaultDate'] = queryPair[1];
			}
			break;
		}
	}

	$.datepicker.setDefaults($.datepicker.regional['ru']);

	$(document).ready(function() {
		var yearRange = '-2:+0';
		if (window.availableEventsYears !== undefined) {
			yearRange = window.availableEventsYears[0] + ':' + window.availableEventsYears[availableEventsYears.length - 1];
		}

		var options = {
			changeYear: true,
			showOtherMonths: true,
			yearRange: yearRange,
			onSelect: function(date) {
				setTimeout(function() {
					$(".events__calendar .ui-datepicker-year").wrap('<div class="ui-datepicker-year-wrap"></div>');
				}, 20);
				if (!lock) {
					lock = true;
					var url = $(".events__calendar").data('url');
					window.location.href = url + '?date=' + date + '#list';
				}
			},
			onChangeMonthYear: function() {
				setTimeout(function() {
					$(".events__calendar .ui-datepicker-year").wrap('<div class="ui-datepicker-year-wrap"></div>');
				}, 20);
			}
		};
		if (window.availableEventsDats) {
			options['beforeShowDay'] = function(date) {
				var string = jQuery.datepicker.formatDate('dd.mm.yy', date);
				if (window.availableEventsDats.indexOf(string) != -1) {
					return [true, 'ui-state-active', ''];
				} else {
					return [false];
				}
			};
		}
		$(".events__calendar").datepicker(options);
		$(".events__calendar .ui-datepicker-year");
		if (window.availableEventsDats) {
			options['beforeShowDay'] = function (date) {
				var string = jQuery.datepicker.formatDate('dd.mm.yy', date);
				if (window.availableEventsDats.indexOf(string) != -1) {
					return [true, 'ui-state-active', ''];
				} else {
					return [false];
				}
			};
		}
		$(".events__calendar").datepicker(options);
		$(".events__calendar .ui-datepicker-year").wrap('<div class="ui-datepicker-year-wrap"></div>');
	});
})();

/* End */
