; /* Start:"a:4:{s:4:"full";s:96:"/local/templates/index/components/bitrix/system.auth.forgotpasswd/popup/script.js?15601600231318";s:6:"source";s:81:"/local/templates/index/components/bitrix/system.auth.forgotpasswd/popup/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(function() {
	var popup = $('#pop-up__forgot'),
		mainForm = $('[name="popup_forgot_password"]'),
		isSubmitLocked = false;

	mainForm.validate({
		rules: {
			"USER_EMAIL": {
				emailBitrix: true,
				required: true
			}
		},
		highlight: function(element, errorClass, validClass) {
			$(element).addClass('error pop-up--field-error');
			mainForm.find('.form-errors').empty();
		},
		unhighlight: function(element, errorClass, validClass) {
			$(element).removeClass('error pop-up--field-error');
			mainForm.find('.form-errors').empty();
		},
		submitHandler: function (form) {
			var $form = $(form);
			if (isSubmitLocked) {
				return false;
			}
			isSubmitLocked = true;

			$.ajax({
				url: '/local/ajax/forgot_password.php',
				method: $form.attr('method'),
				data: $form.serialize()
			})
			.done(function (response) {
				if (response.status !== 'ok') {
					$form.find('.form-errors')
						.html('<label class="error">' + response.message + '</label>')
						.show();

					$form.find(".pop-up__field-wrap")
						.children('input').addClass("pop-up--field-error");

					return false;
				}
				$('.forgot-pwd').html('<p>' + response.message + '</p>');
				popup.addClass('pop-up__login_success');
			})
			.always(function () {
				isSubmitLocked = false;
			});

			return false;
		}
	});
});

/* End */
