//! moment.js locale configuration
//! locale : Russian [ru]
//! author : Viktorminator : https://github.com/Viktorminator
//! Author : Menelion Elensúle : https://github.com/Oire
//! author : Коренберг Марк : https://github.com/socketpair

;(function (global, factory) {
   typeof exports === 'object' && typeof module !== 'undefined'
       && typeof require === 'function' ? factory(require('../moment')) :
   typeof define === 'function' && define.amd ? define(['../moment'], factory) :
   factory(global.moment)
}(this, (function (moment) { 'use strict';


function plural(word, num) {
    var forms = word.split('_');
    return num % 10 === 1 && num % 100 !== 11 ? forms[0] : (num % 10 >= 2 && num % 10 <= 4 && (num % 100 < 10 || num % 100 >= 20) ? forms[1] : forms[2]);
}
function relativeTimeWithPlural(number, withoutSuffix, key) {
    var format = {
        'mm': withoutSuffix ? 'минута_минуты_минут' : 'минуту_минуты_минут',
        'hh': 'час_часа_часов',
        'dd': 'день_дня_дней',
        'MM': 'месяц_месяца_месяцев',
        'yy': 'год_года_лет'
    };
    if (key === 'm') {
        return withoutSuffix ? 'минута' : 'минуту';
    }
    else {
        return number + ' ' + plural(format[key], +number);
    }
}
var monthsParse = [/^янв/i, /^фев/i, /^мар/i, /^апр/i, /^ма[йя]/i, /^июн/i, /^июл/i, /^авг/i, /^сен/i, /^окт/i, /^ноя/i, /^дек/i];

// http://new.gramota.ru/spravka/rules/139-prop : § 103
// Сокращения месяцев: http://new.gramota.ru/spravka/buro/search-answer?s=242637
// CLDR data:          http://www.unicode.org/cldr/charts/28/summary/ru.html#1753
var ru = moment.defineLocale('ru', {
    months : {
        format: 'января_февраля_марта_апреля_мая_июня_июля_августа_сентября_октября_ноября_декабря'.split('_'),
        standalone: 'январь_февраль_март_апрель_май_июнь_июль_август_сентябрь_октябрь_ноябрь_декабрь'.split('_')
    },
    monthsShort : {
        // по CLDR именно "июл." и "июн.", но какой смысл менять букву на точку ?
        format: 'янв._февр._мар._апр._мая_июня_июля_авг._сент._окт._нояб._дек.'.split('_'),
        standalone: 'янв._февр._март_апр._май_июнь_июль_авг._сент._окт._нояб._дек.'.split('_')
    },
    weekdays : {
        standalone: 'воскресенье_понедельник_вторник_среда_четверг_пятница_суббота'.split('_'),
        format: 'воскресенье_понедельник_вторник_среду_четверг_пятницу_субботу'.split('_'),
        isFormat: /\[ ?[Вв] ?(?:прошлую|следующую|эту)? ?\] ?dddd/
    },
    weekdaysShort : 'вс_пн_вт_ср_чт_пт_сб'.split('_'),
    weekdaysMin : 'вс_пн_вт_ср_чт_пт_сб'.split('_'),
    monthsParse : monthsParse,
    longMonthsParse : monthsParse,
    shortMonthsParse : monthsParse,

    // полные названия с падежами, по три буквы, для некоторых, по 4 буквы, сокращения с точкой и без точки
    monthsRegex: /^(январ[ья]|янв\.?|феврал[ья]|февр?\.?|марта?|мар\.?|апрел[ья]|апр\.?|ма[йя]|июн[ья]|июн\.?|июл[ья]|июл\.?|августа?|авг\.?|сентябр[ья]|сент?\.?|октябр[ья]|окт\.?|ноябр[ья]|нояб?\.?|декабр[ья]|дек\.?)/i,

    // копия предыдущего
    monthsShortRegex: /^(январ[ья]|янв\.?|феврал[ья]|февр?\.?|марта?|мар\.?|апрел[ья]|апр\.?|ма[йя]|июн[ья]|июн\.?|июл[ья]|июл\.?|августа?|авг\.?|сентябр[ья]|сент?\.?|октябр[ья]|окт\.?|ноябр[ья]|нояб?\.?|декабр[ья]|дек\.?)/i,

    // полные названия с падежами
    monthsStrictRegex: /^(январ[яь]|феврал[яь]|марта?|апрел[яь]|ма[яй]|июн[яь]|июл[яь]|августа?|сентябр[яь]|октябр[яь]|ноябр[яь]|декабр[яь])/i,

    // Выражение, которое соотвествует только сокращённым формам
    monthsShortStrictRegex: /^(янв\.|февр?\.|мар[т.]|апр\.|ма[яй]|июн[ья.]|июл[ья.]|авг\.|сент?\.|окт\.|нояб?\.|дек\.)/i,
    longDateFormat : {
        LT : 'HH:mm',
        LTS : 'HH:mm:ss',
        L : 'DD.MM.YYYY',
        LL : 'D MMMM YYYY г.',
        LLL : 'D MMMM YYYY г., HH:mm',
        LLLL : 'dddd, D MMMM YYYY г., HH:mm'
    },
    calendar : {
        sameDay: '[Сегодня в] LT',
        nextDay: '[Завтра в] LT',
        lastDay: '[Вчера в] LT',
        nextWeek: function (now) {
            if (now.week() !== this.week()) {
                switch (this.day()) {
                    case 0:
                        return '[В следующее] dddd [в] LT';
                    case 1:
                    case 2:
                    case 4:
                        return '[В следующий] dddd [в] LT';
                    case 3:
                    case 5:
                    case 6:
                        return '[В следующую] dddd [в] LT';
                }
            } else {
                if (this.day() === 2) {
                    return '[Во] dddd [в] LT';
                } else {
                    return '[В] dddd [в] LT';
                }
            }
        },
        lastWeek: function (now) {
            if (now.week() !== this.week()) {
                switch (this.day()) {
                    case 0:
                        return '[В прошлое] dddd [в] LT';
                    case 1:
                    case 2:
                    case 4:
                        return '[В прошлый] dddd [в] LT';
                    case 3:
                    case 5:
                    case 6:
                        return '[В прошлую] dddd [в] LT';
                }
            } else {
                if (this.day() === 2) {
                    return '[Во] dddd [в] LT';
                } else {
                    return '[В] dddd [в] LT';
                }
            }
        },
        sameElse: 'L'
    },
    relativeTime : {
        future : 'через %s',
        past : '%s назад',
        s : 'несколько секунд',
        m : relativeTimeWithPlural,
        mm : relativeTimeWithPlural,
        h : 'час',
        hh : relativeTimeWithPlural,
        d : 'день',
        dd : relativeTimeWithPlural,
        M : 'месяц',
        MM : relativeTimeWithPlural,
        y : 'год',
        yy : relativeTimeWithPlural
    },
    meridiemParse: /ночи|утра|дня|вечера/i,
    isPM : function (input) {
        return /^(дня|вечера)$/.test(input);
    },
    meridiem : function (hour, minute, isLower) {
        if (hour < 4) {
            return 'ночи';
        } else if (hour < 12) {
            return 'утра';
        } else if (hour < 17) {
            return 'дня';
        } else {
            return 'вечера';
        }
    },
    dayOfMonthOrdinalParse: /\d{1,2}-(й|го|я)/,
    ordinal: function (number, period) {
        switch (period) {
            case 'M':
            case 'd':
            case 'DDD':
                return number + '-й';
            case 'D':
                return number + '-го';
            case 'w':
            case 'W':
                return number + '-я';
            default:
                return number;
        }
    },
    week : {
        dow : 1, // Monday is the first day of the week.
        doy : 7  // The week that contains Jan 1st is the first week of the year.
    }
});

return ru;

})));

/* End */
;
; /* Start:"a:4:{s:4:"full";s:64:"/local/templates/index/js/component-fc-tooltip.js?15601600231601";s:6:"source";s:49:"/local/templates/index/js/component-fc-tooltip.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
Vue.component('FcTooltip', {
	name: 'FcTooltip',
	data: function () {
		return {
			iconClass: '',
			textStyle: {}
		};
	},
	methods: {
		setPosition: function () {
			var textWidth = 280;
			var iconPosition = this.$refs.icon.getBoundingClientRect();
			var iconCenterX = parseInt(iconPosition.left, 10) + 8;
			var iconCenterY = parseInt(iconPosition.top, 10) + 8;
			var directionX = iconCenterX < (window.innerWidth / 2) ? 'left' : 'right';
			var directionY = iconCenterY < (window.innerHeight / 2) ? 'top' : 'bottom';

			this.iconClass = directionY === 'top' ? 'fc-tooltip__icon_top' : 'fc-tooltip__icon_bottom';
			this.textStyle = {};

			// Horizontal position
			var left = iconCenterX - 20;
			this.textStyle.left = left + 'px';

			if (left + textWidth > window.innerWidth) {
				this.textStyle.left = window.innerWidth - textWidth - 20 + 'px';
			}

			// Vertical position
			if (directionY === 'top') {
				this.textStyle.top = iconCenterY + 20 + 'px';
			} else {
				this.textStyle.bottom = (window.innerHeight - iconCenterY) + 20 + 'px';
			}
		}
	},
	destroyed: function () {
		window.removeEventListener('resize', this.setPosition);
		window.removeEventListener('scroll', this.setPosition);
	},
	mounted: function () {
		window.addEventListener('resize', this.setPosition);
		window.addEventListener('scroll', this.setPosition);
		this.setPosition()
	},
	template: [
		'<div class="fc-tooltip">',
		'  <a class="fc-tooltip__icon" v-bind:class="iconClass" ref="icon">?</a>',
		'  <p class="fc-tooltip__text" v-bind:style="textStyle"><slot></slot></p>',
		'</div>'
	].join('')
});

/* End */
;
; /* Start:"a:4:{s:4:"full";s:65:"/local/templates/index/js/directive-v-datepicker.js?1560160023668";s:6:"source";s:51:"/local/templates/index/js/directive-v-datepicker.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
Vue.directive('datepicker', {
	bind: function (el) {
		$(el).datepicker({
			nextText: '<i class="fa fa-angle-right"></i>',
			prevText: '<i class="fa fa-angle-left"></i>',
			beforeShow: function(input, instance) {
				$(input).after($(input).datepicker('widget'));
				instance.dpDiv.addClass('datepicker');
			},
			onSelect: function (date) {
				el.value = date;

				var inputEvent = document.createEvent('HTMLEvents');
				inputEvent.initEvent('input', true, true);
				el.dispatchEvent(inputEvent);

				var changeEvent = document.createEvent('HTMLEvents');
				changeEvent.initEvent('change', true, true);
				el.dispatchEvent(changeEvent);
			}
		});
	}
});

/* End */
;
; /* Start:"a:4:{s:4:"full";s:54:"/local/templates/index/js/filter-date.js?1560160023181";s:6:"source";s:40:"/local/templates/index/js/filter-date.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
Vue.filter('date', function (input, outputFormat, inputFormat) {
	return inputFormat
		? moment(input, inputFormat).format(outputFormat)
		: moment(input).format(outputFormat);
});

/* End */
;
; /* Start:"a:4:{s:4:"full";s:61:"/local/templates/index/js/mixin-calculators.js?15601600236916";s:6:"source";s:46:"/local/templates/index/js/mixin-calculators.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
var calculatorsMixin = {
	computed: {
		fieldDateOpening: {
			get: function () {
				return this.fields.dateOpening;
			},
			set: function (value) {
				var date = this.toAvailableDateFormat(value);
				if (moment(date, 'DD.MM.YYYY').isValid()) {
					this.fields.dateOpening = date;
				}
			}
		},
		fieldDateClosing: {
			get: function () {
				return this.fields.dateClosing;
			},
			set: function (value) {
				var date = this.toAvailableDateFormat(value);
				if (moment(date, 'DD.MM.YYYY').isValid()) {
					this.fields.dateClosing = date;
				}
			}
		},
		fieldAmount: {
			get: function () {
				return this.numberToString(this.fields.amount, 2);
			},
			set: function (value) {
				this.fields.amount = Math.abs(this.stringToNumber(value, 2)) || 0;
			}
		},
		fieldPayment: {
			get: function () {
				return this.numberToString(this.fields.payment, 2);
			},
			set: function (value) {
				this.fields.payment = Math.abs(this.stringToNumber(value, 2)) || 0;
			}
		},
		fieldRate: {
			get: function () {
				return this.numberToString(this.fields.rate, 2);
			},
			set: function (value) {
				this.fields.rate = Math.abs(this.stringToNumber(value, 2)) || 0;
			}
		},
		fieldRateEarlyClosing: {
			get: function () {
				return this.numberToString(this.fields.rateEarlyClosing, 2);
			},
			set: function (value) {
				this.fields.rateEarlyClosing = Math.abs(this.stringToNumber(value, 2)) || 0;
			}
		},
		fieldLoanTermCount: {
			get: function () {
				return this.numberToString(this.fields.loanTermCount, 0);
			},
			set: function (value) {
				this.fields.loanTermCount = Math.abs(this.stringToNumber(value, 0)) || 0;
			}
		},

		dateOpening: function () {
			return moment(this.fields.dateOpening, 'DD.MM.YYYY');
		},
		dateClosing: function () {
			var closing = this.fields.isEarlyClosing
				? moment(this.fields.dateClosing, 'DD.MM.YYYY')
				: moment(this.fields.dateOpening, 'DD.MM.YYYY').add(this.fields.loanTermCount, this.fields.loanTermUnits);

			return this.toAvailableDate(closing);
		},
		periods: function () {
			if (this.periodicity === 'one-time') {
				return [{
					dateOpening: this.dateOpening,
					dateClosing: this.dateClosing,

					daysInPeriod: this.getDays(this.dateOpening, this.dateClosing),
					daysInPeriodYear: (this.getDaysInYear(this.dateOpening) + this.getDaysInYear(this.dateClosing)) / 2
				}];
			}

			var periods = [];
			var periodDateOpening = moment(this.dateOpening);
			var periodDateClosing;

			// Дробим на периоды
			for (var i = 1; i > 0; i++) {
				periodDateClosing = this.toAvailableDate(moment(this.dateOpening).add(i, this.periodicity));

				// Последний период
				if (periodDateClosing >= this.dateClosing) {
					periodDateClosing = moment(this.dateClosing);
					i = -1;
				}

				periods.push({
					dateOpening: periodDateOpening,
					dateClosing: periodDateClosing,

					daysInPeriod: this.getDays(periodDateOpening, periodDateClosing),
					daysInPeriodYear: (this.getDaysInYear(periodDateOpening) + this.getDaysInYear(periodDateClosing)) / 2
				});

				// Следующий период начнем с окончания текущего
				periodDateOpening = moment(periodDateClosing);
			}

			return periods;
		},
		rate: function () {
			return this.fields.isEarlyClosing ? this.fields.rateEarlyClosing : this.fields.rate;
		},

		loanTermCountMax: function () {
			switch (this.fields.loanTermUnits) {
				case 'days':
					return 50 * 365;
				case 'months':
					return 50 * 12;
				case 'years':
					return 50;
			}
		},
		loanTermCountMin: function () {
			return this.fields.loanTermUnits === 'days' ? 30 : 1;
		},
		dateClosingMax: function () {
			return moment(this.dateOpening).add(this.fields.loanTermCount, this.fields.loanTermUnits).format('DD.MM.YYYY');
		},
		dateClosingMin: function () {
			return moment(this.dateOpening).add(1, 'days').format('DD.MM.YYYY');
		},
		datePaymentsMax: function () {
			return moment(this.dateClosing).subtract(1, 'days').format('DD.MM.YYYY');
		},
		datePaymentsMin: function () {
			return this.dateOpening.format('DD.MM.YYYY');
		}
	},
	data: {
		fields: {
			dateOpening: moment().format('DD.MM.YYYY'),
			dateClosing: moment().add(1, 'years').format('DD.MM.YYYY'),

			loanTermCount: 1,
			loanTermUnits: 'years',

			currency: 'RUB',
			periodicity: 'months',

			rate: 10,
			rateEarlyClosing: 0,

			isEarlyClosing: false,
			isPostpone: false
		},
		state: {},

		notifyList: []
	},
	methods: {
		numberToString: function (number, count) {
			count = count !== undefined ? count : 2;
			number = count ? parseFloat(number || 0) : parseInt(number || 0, 10);

			return number
				.toFixed(count)
				.replace(/\./g, ',')
				.replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1 ');
		},
		stringToNumber: function (string, count) {
			string = string.replace(/\s/g, '').replace(/\,/g, '.');

			return count
				? parseFloat(parseFloat(string).toFixed(count))
				: parseInt(string, 10);
		},
		toFixedNumber: function (value, count) {
			return count
				? parseFloat(parseFloat(value).toFixed(count))
				: parseInt(value, 10);
		},
		toAvailableDate: function (value) {
			var newValue = moment(value);

			if (!newValue.isValid()) return value;
			if (this.fields.isPostpone && newValue.isoWeekday() === 6) newValue.add(2, 'days');
			if (this.fields.isPostpone && newValue.isoWeekday() === 7) newValue.add(1, 'days');

			return newValue;
		},
		toAvailableDateFormat: function (value) {
			return this.toAvailableDate(moment(value, 'DD.MM.YYYY')).format('DD.MM.YYYY');
		},

		getDays: function (opening, closing) {
			return (new Date(closing) - new Date(opening)) / 864e5;
		},
		getDaysInYear: function (date) {
			return new Date(date).getFullYear() % 4 ? 365 : 366;
		},

		print: function () {
			window.print();
		},
		showErrorMessage: function (message) {
			swal({
				text: '<div class="calc__alert-icon"><svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="23.983px" height="70.994px" viewBox="0 0 23.983 70.994" enable-background="new 0 0 23.983 70.994" xml:space="preserve"><path fill-rule="evenodd" clip-rule="evenodd" fill="#FFFFFF" d="M0,61.014h7.24V31.072H0v-9.981h12.037c2.625,0,4.797,2.26,4.797,4.99v34.932h7.149v9.98H0V61.014z M12.037,0c3.982,0,7.149,3.295,7.149,7.438s-3.167,7.438-7.149,7.438c-3.981,0-7.24-3.295-7.24-7.438S8.055,0,12.037,0L12.037,0z"/></svg></div><div class="calc__alert-text">' + message + '</div>',
				showCloseButton: true,
				showConfirmButton: false,
				padding: 0,
				width: '420px'
			});
		}
	},
	watch: {
		fields: { deep: true, handler: 'clearResult' },
		'fields.isPostpone': function (value) {
			var self = this;

			self.fields.dateOpening = self.toAvailableDateFormat(self.fields.dateOpening);
			self.fields.dateClosing = self.toAvailableDateFormat(self.fields.dateClosing);
		}
	}
};

/* End */
;
; /* Start:"a:4:{s:4:"full";s:58:"/local/templates/index/js/filter-to-fixed.js?1560160023277";s:6:"source";s:44:"/local/templates/index/js/filter-to-fixed.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
Vue.filter('toFixed', function (number, count) {
	number = parseFloat(number);

	if (number.toFixed(2) === '0.00' || number.toFixed(2) === '-0.00') {
		return 0;
	}

	return number
		? count
			? parseFloat(number).toFixed(count)
			: parseFloat(number).toFixed(2)
		: '';
});

/* End */
;
; /* Start:"a:4:{s:4:"full";s:70:"/local/templates/index/js/filter-to-fixed-with-spaces.js?1560160023363";s:6:"source";s:56:"/local/templates/index/js/filter-to-fixed-with-spaces.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
Vue.filter('toFixedWithSpaces', function (number, count) {
	number = parseFloat(number);

	if (number.toFixed(2) === '0.00' || number.toFixed(2) === '-0.00') {
		return 0;
	}

	return number
		? count
			? parseFloat(number).toFixed(count).replace(/\B(?=(\d{3})+(?!\d))/g, " ")
			: parseFloat(number).toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, " ")
		: '';
});

/* End */
