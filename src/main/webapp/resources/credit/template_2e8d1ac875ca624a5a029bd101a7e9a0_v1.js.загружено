; /* Start:"a:4:{s:4:"full";s:48:"/local/templates/index/js/tabs.js?15601600231418";s:6:"source";s:33:"/local/templates/index/js/tabs.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
/*global $*/

(function() {

	$(document).ready(function() {
		var tabsContent = $(".tabs__content");
		var tabsLink = $(".tabs__list").find("li");
		var activeClass = "_active";
		var tabsWrap = $(".tabs__wrap");
		var tabsContentWrap = $(".tabs-content-wrap");


		tabsLink.on("click", function(e) {
			e.preventDefault();
			$(tabsLink).removeClass(activeClass);
			$(this).addClass(activeClass);
			$.each(tabsLink, function(key, value) {
				if ($(tabsLink[key]).hasClass(activeClass)) {
					$(tabsContent).removeClass(activeClass);
					$(tabsContent[key]).addClass(activeClass);
				}
			});
		});
		if ($(window).width()<767) {
				$(".tabs__wrap").mCustomScrollbar({ axis:"x"});
		}

		$(window).on("scroll", function(e) {
			if (tabsWrap.length === 0) return;
			if (window.pageYOffset < tabsWrap[0].getBoundingClientRect().top + window.pageYOffset) {
				if (tabsWrap.hasClass("tabs__wrap_fixed")) {
					tabsWrap.removeClass("tabs__wrap_fixed");
					tabsWrap[0].style.width = "auto";
				}
			}
			if (tabsWrap[0].getBoundingClientRect().top <= 55
				&& tabsContentWrap[0].getBoundingClientRect().bottom >= 75) {
				if (!tabsWrap.hasClass("tabs__wrap_fixed")) {
					tabsWrap.addClass("tabs__wrap_fixed");
					tabsWrap[0].style.width = tabsContentWrap[0].offsetWidth + "px";
				}
			} else {
				tabsWrap.removeClass("tabs__wrap_fixed");
				tabsWrap[0].style.width = "auto";
			}
		});
	});


})();

/* End */
;
; /* Start:"a:4:{s:4:"full";s:50:"/local/templates/index/js/helper.js?15601600232195";s:6:"source";s:35:"/local/templates/index/js/helper.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
/*global $*/

(function() {

	$(document).ready(function() {

		var
			cache = {},
			timeout = 0,
			form = $('#helper-search-form'),
			input = form.find('#search-helper'),
			autoComplete = form.find('.search_result');

		var showResult = function(data) {
			autoComplete.html(data);
			if (!autoComplete.is(':visible')) {
				autoComplete.stop(true, true).fadeIn(300);
				input.addClass("_active");
			}
		};

		var hideResult = function() {
			autoComplete.stop(true, true).fadeOut(300);
			input.removeClass("_active");
		};


		var search = function() {
			clearTimeout(timeout);
			timeout = setTimeout(function() {
				var query = $.trim(input.val());
				if (query.length > 2) {
					if (cache[query]) {
						showResult(cache[query]);
					} else {
						$.get('/local/ajax/search.php?type=suggest', {
							q: query
						}, function(obj) {
							cache[query] = obj;
							showResult(obj);
						}, 'html');
					}
				} else {
					hideResult();
				}
			}, 300);
		};

		autoComplete.on('click.autoComplete', function(e) {
			e.stopPropagation();
		});

		input
			.on('keydown', function() {
				search();
			})
			.on('change', function() {
				search();
			})
			.on('focus', function() {
				search();
			})
			.on('keyup', function (e) {
				if (e.keyCode === '13') {
					form.submit();
				}
			});

		$(window).trigger("resize");
		$(document)
			.on('click.autoComplete', function() {
				hideResult();
			})
			.on('click', '.index-search-btn', function() {
				form.submit();
			});

		var helperContent = $(".helper__mid").children();
		var activeClass = "_active";
		var searchField = $("#search-helper");
		var tabsLink = $(".helper__link-block").find("a");

		tabsLink.on("click", function(e) {
			e.preventDefault();
			if (this.text === 'Популярное') {
				yaCounter45637635.reachGoal('popular-section');
			}
			$(tabsLink).removeClass(activeClass);
			$(e.target).addClass(activeClass);
			$.each(tabsLink, function(key, value) {
				if ($(tabsLink[key]).hasClass(activeClass)) {
					$(helperContent).removeClass(activeClass);
					$(helperContent[key]).addClass(activeClass);
					$('.index-search-result').hide();
				}
			});
		});
	});

})();

/* End */
;
; /* Start:"a:4:{s:4:"full";s:50:"/local/templates/index/js/pop-up.js?15601600233009";s:6:"source";s:35:"/local/templates/index/js/pop-up.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
/*global $, console*/

(function() {
	"use strict";

	var startWindowScroll = 0;

	if ($('a[name="banner__pop-up"]').length > 0) {
		$('a[name="banner__pop-up"]')
			.magnificPopup({
				fixedContentPos: true,
				fixedBgPos: true,
				overflowY: 'auto',
				modal: false,
				showCloseBtn: false,
				closeOnBgClick: true,
				callbacks: {
					beforeOpen: function() {
						startWindowScroll = $(window).scrollTop();
					},
					open: function(){
						if ( $('.mfp-content').height() < $(window).height() ){
							$('body').on('touchmove', function (e) {
								e.preventDefault();
							});
						}
					},
					close: function () {
						$(window).scrollTop(startWindowScroll);
						$('body').off('touchmove');

						if ($('.promo__media').length > 0) {
							var video = $('.pop-up__promo-video').find('video')[0];
							if (typeof video !== 'undefined') {
								video.pause();
								console.log('pause');
							}
						}
					}
				}
			})
		;
	}

	var popup_fields = $('.pop-up__advantages-link');
	popup_fields.magnificPopup({
		fixedContentPos: true,
		fixedBgPos: true,
		closeOnBgClick: true,
		overflowY: 'auto',
		showCloseBtn: false,
		callbacks: {
			beforeOpen: function() {
				startWindowScroll = $(window).scrollTop();
			},
			open: function(){
				if ( $('.mfp-content').height() < $(window).height() ){
					$('body').on('touchmove', function (e) {
						e.preventDefault();
					});
				}
			},
			close: function() {
				$(window).scrollTop(startWindowScroll);
				$('body').off('touchmove');

				$('.pop-up__wrap input[type=password]').val('');
			}
		}
	});

	// Попап входа на сайт
	$('.header__auth-login, .go-login-popup, .footer__auth-link, .go-register-popup, .pop-up__auth-forgot').magnificPopup({
		fixedContentPos: true,
		fixedBgPos: true,
		closeOnBgClick: true,
		overflowY: 'auto',
		showCloseBtn: false,
		callbacks: {
			beforeOpen: function() {
				startWindowScroll = $(window).scrollTop();
			},
			open: function(){
				if ( $('.mfp-content').height() < $(window).height() ){
					$('body').on('touchmove', function (e) {
						e.preventDefault();
					});
				}
			},
			close: function() {
				$(window).scrollTop(startWindowScroll);
				$('body').off('touchmove');

				$('.pop-up__wrap form').each(function (i, form) {
					var validator = $(form).validate();
					validator.resetForm();
					$(form).find(':text').removeClass('error');
					//$(form).find('.form-error').hide();
					$(form).find('.pop-up__field-wrap').children('input').removeClass('pop-up--field-error');
				});
			}
		}
	});

	popup_fields.on('mfpOpen', function() {
		$('.pop-up__wrap input[type=password]').val('');
	});

	$(document).on('click', '.popup-modal-dismiss', function(e) {
		e.preventDefault();
		$.magnificPopup.close();
		$('.pop-up__wrap input[type=password]').val('');
	});

	$(".pop-up__features-title").on("click", function() {
		!$(this).hasClass("_active") ? $(this).addClass("_active") : $(this).removeClass("_active");
	});



})();

/* End */
