		'									<input v-else type="text" class="vote__answers-text" :id="\'vote_field_\' + a.ID" :name="\'vote_field_\' + a.ID" value="" :size="a.FIELD_WIDTH"/>\n',
		'								</label>\n',
		'							</span>\n',
		'							<span v-if="a.FIELD_TYPE == 5" class="vote-answer-item vote-answer-item-memo">\n',
