; /* Start:"a:4:{s:4:"full";s:48:"/local/templates/index/js/news.js?15601600231880";s:6:"source";s:33:"/local/templates/index/js/news.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
/*global $*/

(function () {

	$(document).ready(function () {

		var news = $(".news");
		var newsWrap = $(".news__wrap");
		var moreNewsLink = $(".news__more");
		var newsItem = $(".news__item");
		var newsItemTitle = $(".news").find("h3");
		var activeClass = "_active";


		newsItemTitle.on("click", function (e) {
			e.preventDefault();
			if ($(this).parent().hasClass(activeClass)) {
				window.location = e.target.href;
				return;
			}
			var titles = newsWrap.find(newsItem);
			$.each(titles, function (key, value) {
				if (!$(value).hasClass(activeClass)) {
					$(value).addClass(activeClass);
					return;
				}
				if ($(value).hasClass(activeClass)) {
					$(value).removeClass(activeClass);
				}
			});
		});

		var newsPreviewItem = $('.news__preview').find('li');
		var newsListItem = $('.news__list').find('li');

		var fetchItems = function (obj, block, flag) {

			if(obj.length === 0) {
				setWrapHeight(block, 0);
				return
			}

			if (!fetchItems.flag) {
				fetchItems.flag = true;
			}

			$.each(obj, function (i, val) {
				if (i > 4) {
					return
				}

				getOverallHeight(val, fetchItems.flag);
			});

			// setWrapHeight(block, getOverallHeight.overallHeight - 60);
		}

		function getOverallHeight(item, flag) {

			if (!getOverallHeight.overallHeight) {
				getOverallHeight.overallHeight = 0;
			}

			if (flag) {
				getOverallHeight.overallHeight = 0;
				fetchItems.flag = false;
			}

			getOverallHeight.overallHeight += $(item).outerHeight();
		}

		var setWrapHeight = function (block, height) {
			block.height(height);
		}

		$(window).resize(function() {
			if($(window).width() > 767) {
				$('.news__preview').removeAttr('style');
				$('.news__list').removeAttr('style');
				return
			}
			fetchItems(newsPreviewItem, $('.news__preview'));
			fetchItems(newsListItem, $('.news__list'), fetchItems.flag);
		})

	});

})();

/* End */
